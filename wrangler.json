{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "landworks",
	"main": "./workers/app.ts",
	"compatibility_date": "2025-10-08",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true,
	"vars": {
		"ENVIRONMENT": "production",
		"API_URL": "https://api.desiletslandworks.com"
	},
	"routes": [
		{
			"pattern": "desiletslandworks.com/*",
			"zone_name": "desiletslandworks.com"
		},
		{
			"pattern": "www.desiletslandworks.com/*",
			"zone_name": "desiletslandworks.com"
		}
	],
	"env": {
		"staging": {
			"name": "landworks-staging",
			"workers_dev": true,
			"vars": {
				"ENVIRONMENT": "staging",
				"API_URL": "https://staging-api.desiletslandworks.com"
			}
		}
	}
}